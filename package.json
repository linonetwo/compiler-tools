{
  "name": "cheatsheet",
  "version": "0.1.0",
  "title": "cheatsheet",
  "description": "An offline cheatsheet , young and simple , using the faceted searching to help you explore.",
  "keywords": [
    "cheatsheet",
    "React"
  ],
  "author": {
    "name": "linonetwo",
    "url": "linonetwo.github.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/linonetwo/cheatsheet"
  },
  "private": false,
  "dependencies": {
    "@blueprintjs/core": "^1.14.0",
    "bluebird": "^3.5.0",
    "first-follow": "^1.2.0",
    "flow-runtime": "^0.10.0",
    "gun": "^0.7.1",
    "lodash": "^4.17.4",
    "react": "^15.5.3",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.3",
    "react-markdown": "^2.4.6",
    "react-measure": "^1.4.6",
    "styled-components": "^1.4.4",
    "synograph": "^0.4.0",
    "viz.js": "^1.8.0"
  },
  "devDependencies": {
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "del": "^2.2.2",
    "eslint": "3.16.1",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "6.4.1",
    "eslint-plugin-standard": "^3.0.1",
    "fs-promise": "^2.0.2",
    "gulp": "^3.9.1",
    "gulp-inline-source": "^3.1.0",
    "gulp-replace": "^0.5.4",
    "gulp-run": "^1.7.1",
    "autoprefixer": "6.7.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-react-app": "^2.2.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "1.6.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "react-dev-utils": "^0.5.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      ["babel-plugin-transform-builtin-extend", {
        "globals": ["Error", "Set"]
      }]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
